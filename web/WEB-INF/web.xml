<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>com.xu.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User1</servlet-name>
        <servlet-class>com.xu.User1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User2</servlet-name>
        <servlet-class>com.xu.User2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User1</servlet-name>
        <url-pattern>/User1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User2</servlet-name>
        <url-pattern>/User2</url-pattern>
    </servlet-mapping>


</web-app>
